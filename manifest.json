{
	"name": "Productivity Timer",
	"version": "1.0",
	"manifest_version": 2,
	"description": "Keeps track of your browsing time",
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_end"
      }
	],
	"permissions":[
        "activeTab",
		"storage"
	]


}