{
	"name": "Productivity Timer",
	"version": "1.0",
	"manifest_version": 2,
	"description": "Keeps track of your browsing time",
	"permissions": ["activeTab","declarativeContent", "storage"],

	"browser_action": {
		"default_icon": "icon.png"
	},

	
	"background" : {
    	"scripts" : ["scripts/popup.js"]
  	}

}